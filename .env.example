# LLM Configuration - Using Gemini
LLM_PROVIDER=gemini
LLM_API_KEY=your_gemini_api_key_here
LLM_BASE_URL=https://generativelanguage.googleapis.com/v1beta
LLM_MODEL=gemini-pro

# Alternative Gemini models:
# LLM_MODEL=gemini-1.5-pro
# LLM_MODEL=gemini-1.5-flash

# For other providers (not currently supported in UI):
# OpenAI: LLM_PROVIDER=openai, LLM_BASE_URL=https://api.openai.com/v1, LLM_MODEL=gpt-4
# Claude: LLM_PROVIDER=claude, LLM_BASE_URL=https://api.anthropic.com/v1, LLM_MODEL=claude-3-sonnet-20240229
# LM Studio: LLM_PROVIDER=lmstudio, LLM_BASE_URL=http://192.168.1.116:1234/v1, LLM_MODEL=local-model

# Server Configuration
PORT=5000
NODE_ENV=development

# Frontend URL
FRONTEND_URL=http://localhost:3000

# Email Configuration (for email notifications)
EMAIL_HOST=smtp.example.com
EMAIL_PORT=587
EMAIL_SECURE=false
EMAIL_USER=your-email@example.com
EMAIL_PASS=your-app-password
EMAIL_FROM=intermediator@yourdomain.com
